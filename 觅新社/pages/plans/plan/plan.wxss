@import "calendartest";
.tab-nav{
  width: 100%;
  height: 100rpx;
  border-bottom: 1rpx solid #f5f5f5;
  background: #fff;
}
.tab-nav .li{
  /* width:91rpx; */
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 5rpx solid #fff;
  box-sizing: border-box;
  color: #969699;
  font-size: 30rpx;
  margin: 0 73rpx;
}
.tab-nav .cur{
  color: #323233;
  border-bottom: 5rpx solid #000;
}
.content{
  padding: 30rpx 20rpx;
  margin-bottom: 20rpx;
}
.dt{
  font-size: 32rpx;
  font-weight: bold;
  color: #323233;
  margin-bottom: 30rpx;
}
.dd{
  overflow: hidden;
}
.dd>view{
  float: left;
}
.dd.type1 view{
  min-width: 130rpx;
  height: 50rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  border: 1rpx solid #646466;
  font-size: 26rpx;
  margin: 0 40rpx 30rpx 0;
  text-align: center;
  line-height: 48rpx;
  color: #646466;
}
.dl{
  margin-bottom: 20rpx;
}
.dd .cur{
  color: #e2320a !important;
  border-color: #e2320a !important;
}
.dd.type2>view{
  width: 100rpx;
  margin-right: 40rpx;
  margin-bottom:10rpx; 
}
.active-content{
  width: 100rpx;
  height: 100rpx;
  border:2rpx solid #323233;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  font-size: 26rpx;
  color: #323233;
  box-sizing: border-box;
  margin-bottom: 12rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.first{
  width: 100rpx;
  text-align: center;
  color: #646466;
  font-size: 26rpx;
}
.select-date{
  font-size: 26rpx;
  color: #323233;
}
.bottom-row{
  width: 100%;
  height: 100rpx;
  padding:10rpx 70rpx 10rpx 60rpx;
  box-sizing: border-box;
  background: #fff; 
}
.bottom-row>navigator{
  width: 260rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 30rpx;
}
.bottom-row>view{
  width: 260rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #969699;
  border:2rpx solid #969699;
}
/* .bottom-row>navigator:nth-child(1){
  color: #969699;
  border:2rpx solid #969699;
} */
.bottom-row>navigator{
  color: #323233;
  border:2rpx solid #323233;
}
.bs-list{
  padding:0 20rpx;
  padding-top: 20rpx;
}
.bs-list .li{
  padding-bottom: 50rpx;
  margin-bottom: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.bs-list .li image{
  width: 710rpx;
  height: 300rpx;
  margin-bottom: 10rpx;
}
.bs-list .li .h3{
  font-size: 32rpx;
  /* font-weight: bold; */
  line-height: 44rpx;
  margin-bottom: 20rpx;
}
.bs-list .li .m{
  font-size:30rpx; 
}
.bs-list .li .m text{
  color: #e2320a;
}
.bs-list .li .mark{
  width: 110rpx;
  height: 34rpx;
  font-size: 26rpx;
  line-height: 34rpx;
  text-align: center;
  border: 1rpx solid #646466;
  color: #646466;
  margin-right:20rpx;
}
/* .bs-list .li .share-ico{
  width: 34rpx;
  height: 34rpx;
} */
.bs-list .li .like-ico{
  width: 34rpx;
  height: 34rpx;
  margin-bottom: 0;
}
.bs-list .li .num{
  font-size:24rpx;
  color: #323233;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5)
}
.calendar{
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -305rpx;
  margin-top: -300rpx;
}
.td>view{
  width:52rpx;
  height: 52rpx;
  margin: 0 auto;
}