.body{
  padding-bottom: 100rpx;
}
.section{
  padding: 20rpx;
}
.li{
  padding: 30rpx 20rpx;
  /* border-bottom: 1rpx solid #f5f5f5; */
  margin-bottom: 20rpx;
}
.li image{
  width: 280rpx;
  height: 200rpx;
  /* margin-right: 20rpx; */
}
.li .h3{
  font-size: 32rpx;
  color:#323233;
  /* margin-bottom: 10rpx; */
}
.li .mark{
  padding: 0 12rpx;
  height: 34rpx;
  font-size: 26rpx;
  line-height: 34rpx;
  text-align: center;
  border: 1rpx solid #646466;
  color: #646466;
  margin-right:20rpx;
}
.share-ico{
  width: 26rpx;
  height: 33rpx;
}
.like-ico{
  width: 32rpx;
  height: 27rpx;
}
.other-row .num{
  font-family: Arial, Helvetica, sans-serif;
  font-size:24rpx;
  color: #323233;
}
.other-row .time{
  font-size: 26rpx;
  color: #323233;
}
.li .info{
  position: relative;
}
.other-row{
  /* position: absolute;
  bottom: 0; */
  width: 100%;
}
.li .like-ico{
  width: 34rpx;
  height: 34rpx;
  margin-bottom: 0;
}
.p .t1{
  color: #323233;
}
.p .t2{
  color: #e2320a;
}
.li .tt{
  font-size: 26rpx;
  color: #969699;
}
.code{
  font-size: 30rpx;
}
.code text:nth-child(1){
  color: #323233;
}
.code text:nth-child(2){
  color: #e2320a;
}
.bottom-row{
  width: 100%;
  height: 90rpx;
  position: fixed;
  bottom: 0;
  padding-top: 10rpx;
  
}
.bottom-row view{
  width: 600rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border: 2rpx solid #323233;
  color: #323233;
  margin: 0 auto;
}
.tilnav .mark{
  display: inline-block;
}
.tilnav{
  margin-bottom: 10rpx;
}