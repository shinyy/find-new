/* pages/my/feedback/feedback.wxss */
.til-row image{
  width: 15rpx;
  height: 14rpx; 
  margin-right: 20rpx;
}
.til-row view{
  color: #323233;
  font-size: 30rpx;
}
.til-row{
  margin-bottom: 22rpx;
}
textarea{
  font-size: 24rpx;
  padding-left: 20rpx;
  /* background: #f5f5f5; */
  width: 100%;
  height: 180rpx;
  box-sizing: border-box;
  line-height: 40rpx;
  display: block;
}
.section.type1{
  /* height: 240rpx; */
  margin: 20rpx 0;
  padding: 30rpx;
  overflow: hidden;
}
.font-num{
  float: right;
  font-size: 24rpx;
  color: #969699;
}
.section.type2{
  padding-top: 20rpx;
  margin-bottom: 30rpx;
}
.til-row2{
  margin-bottom: 20rpx;
  align-items: flex-end;
  padding-left: 20rpx;
}
.til-row2 text:nth-child(1){
  font-size: 30rpx;
  color: #323233;
}
.til-row2 text:nth-child(2){
  font-size: 24rpx;
  color: #969699;
}
.input-row .li{
  border-bottom: 1rpx solid #f5f5f5;
  height: 100rpx;
  padding-left: 20rpx;
}
.til-row.sp{
  margin: 0;
  width: 190rpx;
}
.input-row .li input{
  font-size: 30rpx;
  color:#646466;
}
button{
  width: 690rpx;
  height: 80rpx;
  font-size: 30rpx;
  border: 2rpx solid #323233;
  color: #323233;
  border-radius: 0;
}
.upliad-row{
  overflow: hidden;
}
.img-list{
  height: 160rpx;
}