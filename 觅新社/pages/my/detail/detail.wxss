.section{
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.section .h3{
  font-size:26rpx;
  color: #323233;
  font-weight: bold;
}
.section .h4{
  text-align: center;
  font-size:32rpx;
  color: #323233;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.section .h5{
  text-align: center;
  font-size:26rpx;
  color: #646466;
  margin-bottom: 50rpx;
}
.section .none{
  height: 335rpx;
  text-align: center;
  color: #969699;
  font-size: 26rpx;
  line-height: 335rpx;
}
.til-row{
  margin-bottom: 30rpx;
}
.til-row .prize{
  color:#646466;
  font-size: 26rpx;
}
.tool-row .local{
  width: 34rpx;
  height: 43rpx;
}
.tool-row .phone{
  width: 44rpx;
  height: 43rpx;
}
.tool-row .border{
  height: 50rpx;
  border-left: 2rpx solid #323233;
}
.tool-row {
  padding:0 200rpx;
}
.buslogo{
  width: 88rpx;
  height: 88rpx;
  margin: 20rpx auto 34rpx;
  display: block;
}
.section .txt{
  font-size: 26rpx;
  color: #646466;
  line-height: 50rpx;
}
.prize-list{
  overflow: hidden;
}
.prize-list>view{
  float: left;
  width: 140rpx;
  overflow: hidden;
  margin-bottom: 36rpx;
}
.prize-img{
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0 auto;
  margin-bottom: 12rpx;
}
.prize-img image{
  width: 74rpx;
  height: 74rpx;
  display: block;
  margin: 0 auto;
}
.prize-name{
  font-size: 26rpx;
  color: #323233;
  text-align: center;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.bottom-row{
  width: 100%;
  height: 100rpx;
  background: #fff;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.bottom-row .btn{
  width: 400rpx;
  height: 80rpx;
  font-size: 30rpx;
  color: #323233;
  border: 2rpx solid #323233;
  text-align: center;
  line-height: 80rpx;
  box-sizing: border-box;
  margin-right: 62rpx;
}
.bottom-row image{
  width: 80rpx;
  height: 80rpx;
  margin-right: 55rpx;
}
.section.sp{
  padding: 0;
  padding-bottom: 20rpx;
}