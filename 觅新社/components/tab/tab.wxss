@import "../../app.wxss";
.tab-nav{
  width: 100%;
  height: 100rpx;
  border-bottom: 1rpx solid #f5f5f5;
  position: sticky;
  top: 0;
  background: #fff;
  z-index: 999;
}
.tab-nav .li{
  /* width:91rpx; */
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 5rpx solid #fff;
  box-sizing: border-box;
  color: #969699;
  font-size: 30rpx;
  margin: 0 35rpx;
  text-align: center;
  padding: 0;
}
.tab-nav .cur{
  color: #323233;
  border-bottom: 5rpx solid #000;
}